{
    "manifest_version": 3,
    "name": "Insert Text",
    "version": "0.1.0",

    "action": {
        "default_popup": "popup.html"
    },
    "description": "Insert predefined phrases",

    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "options_page": "options.html",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting",
        "storage"
    ]
}